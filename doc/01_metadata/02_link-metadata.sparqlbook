    "value": "PREFIX sh: <http://www.w3.org/ns/shacl#>\nPREFIX flux: <https://flux.described.at/>\nPREFIX fluxUiMeta: <https://flux.described.at/flux/meta/>\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\n\nCONSTRUCT {\n  ?input a ?class .\n  \n  ?link a flux:Link ;\n    sh:targetClass ?fromClass ;\n    sh:class ?toClass ;\n    sh:name ?name ;\n    sh:path ?path .\n  \n  ?path sh:inversePath ?inversePath .\n\n\n  ?listRest rdf:first ?head ; \n    rdf:rest ?tail .\n} WHERE {\n  {\n    BIND (<https://ld.flux.zazuko.com/role/project-management-coach> AS ?input)\n    ?input a ?class .\n  } UNION\n  {\n    # ---------------------- incoming and outgoing links -----------------------\n    {\n      {\n        SELECT ?link ?fromClass ?toClass ?path WHERE {\n          {\n            SELECT ?fromClass WHERE {\n              BIND (<https://ld.flux.zazuko.com/role/project-management-coach> AS ?input)\n              ?input a ?fromClass .\n            }\n          }\n          ?link sh:targetClass ?fromClass ;\n            sh:class ?toClass ;\n            sh:path ?path ;\n            a flux:Link .\n        }\n      }  \n    } UNION  {\n      {\n        SELECT ?link ?fromClass ?toClass ?path WHERE {\n          {\n            SELECT ?toClass WHERE {\n              BIND (<https://ld.flux.zazuko.com/role/project-management-coach> AS ?input)\n              ?input a ?toClass .\n            }\n          }\n          ?link sh:class ?toClass ;\n            sh:targetClass ?fromClass ;\n            sh:path ?path ;\n            a flux:Link .\n        }\n      }\n    }\n    # ---------------------- incoming and outgoing links -----------------------\n    ?link sh:name ?name  .\n    OPTIONAL {\n      ?path sh:inversePath ?inversePath .\n    }\n  } UNION {\n    # ---------------------- incoming and outgoing links -----------------------\n    {\n      {\n        SELECT ?link ?fromClass ?toClass WHERE {\n          {\n            SEL