  {
    "kind": 2,
    "language": "sparql",
    "value": "PREFIX text: <http://jena.apache.org/text#>\nPREFIX schema: <https://schema.org/>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\nPREFIX xsd: <http://www.w3.org/2001/XMLSchema#>\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\nPREFIX : <https://flux.described.at/>\n\nSELECT ?1_sub ?1_luceneScore WHERE {\n                {\n                  _:e_g_0 text:query _:e_g_2.\n                  _:e_g_2 rdf:first rdfs:comment;\n                    rdf:rest _:e_g_3.\n                  _:e_g_3 rdf:first \"*dns*\";\n                    rdf:rest rdf:nil.\n                  _:e_g_0 rdf:first ?1_sub;\n                    rdf:rest _:e_g_1.\n                  _:e_g_1 rdf:first ?1_luceneScore;\n                    rdf:rest rdf:nil.\n                }\n                UNION\n                {\n                  _:e_g_4 text:query _:e_g_6.\n                  _:e_g_6 rdf:first rdfs:label;\n                    rdf:rest _:e_g_7.\n                  _:e_g_7 rdf:first \"*dns*\";\n                    rdf:rest rdf:nil.\n                  _:e_g_4 rdf:first ?1_sub;\n                    rdf:rest _:e_g_5.\n                  _:e_g_5 rdf:first ?1_luceneScore;\n                    rdf:rest rdf:nil.\n                }\n              }"
